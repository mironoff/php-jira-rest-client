{
    "name": "mironoff/php-jira-rest-client",
    "description": "JIRA REST API Client for PHP Users.",
    "type": "library",
    "keywords": ["jira", "rest", "jira-php", "jira-rest"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mironoff/php-jira-rest-client.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "netresearch/jsonmapper": "^2.0",
        "monolog/monolog": "~1.12|^2.0",
        "symfony/dotenv": "^4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.7 <6",
        "mockery/mockery": "^0.9.4",
        "symfony/var-dumper": "~2.8|~3.0"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "KwangSeob Jeong",
            "email": "lesstif@gmail.com",
            "homepage": "https://lesstif.com/"
        }
    ],
    "autoload": {
        "psr-4" : {
            "mironoff\\JiraRestApi\\" : "src"
        }
    }
}
